@model Prueba_MVC_AP.Models.ClsProducto
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Agregar</title>
</head>
<body>
    <div>
        @using (Html.BeginForm("Agregar", "Producto", FormMethod.Post))
        {

            @Html.Label("Ingrese Producto: ")
            @Html.TextBoxFor(model => model.producto)
            <br />
            @Html.ListBoxFor(x => x.SelectedProIds, Model.ProList, new { style = "width:200px" })
            <br />
            <input type="submit" value="Enviar" />
        }

    </div>
</body>
</html>
